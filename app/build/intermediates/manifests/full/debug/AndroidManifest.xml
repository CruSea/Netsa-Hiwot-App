<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.netsahiwot.netsa_hiwot"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme2" >
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivitySplash"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.netsahiwot.netsa_hiwot.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivityAddictionTest"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivityTempted"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivityTestimonies"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivityGetHelp"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivityContact"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netsahiwot.netsa_hiwot.ActivityAbout"
            android:screenOrientation="portrait" />
    </application>

</manifest>