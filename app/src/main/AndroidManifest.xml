                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserSetting"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.PREFS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActivityAddictionTest"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityTempted"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityTestimonies"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityGetHelp"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityContact"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityAbout"
            android:screenOrientation="portrait" />
        <activity android:name=".ActivityArticles" />
        <receiver android:name=".CustomNotification">
        <intent-filter>
            <action android:name="android.intent.action.BOOT_COMPLETED">
            </action>
        </intent-filter>
        </receiver>
        <!--
